@page "/"

<div>

    <h4>Movie</h4>
    <MoviesList movies="movies"></MoviesList>

</div>

@code
{
    List<Movie> movies;

    protected override void OnInitialized()
    {

        base.OnInitialized();
        movies = new List<Movie>
{
            new Movie(){title = "Spider-Man: Far from home", releaseDate = new DateTime(2019, 7, 8)},
            new Movie(){title = "Joker 2020", releaseDate = new DateTime(2020, 3, 25)},
            new Movie(){title = "Fast and Furious 9", releaseDate = new DateTime(2021, 5, 5)}
        };
    }
}